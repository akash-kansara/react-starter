{
  "name": "react-starter",
  "version": "1.0.0",
  "scripts": {
    "analyse": "cross-env NODE_ENV=production webpack --config webpack/production.js --profile --json > webpack-stats.json",
    "build:dev": "cross-env NODE_ENV=development webpack --config webpack/development.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack/production.js",
    "mock-api": "node json-server",
    "lint": "npm run lint:style&npm run lint:js",
    "lint:js": "eslint \"src/**/*.js\"",
    "lint:style": "stylelint \"src/**/*.scss\" --syntax scss",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack/development.js",
    "start:prod": "serve -s dist",
    "test": "cross-env NODE_ENV=development jest"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.3",
    "babel-jest": "^25.4.0",
    "babel-loader": "^8.1.0",
    "circular-dependency-plugin": "^5.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "command-line-args": "^5.1.1",
    "compression-webpack-plugin": "^3.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^25.4.0",
    "json-server": "^0.16.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "path": "^0.12.7",
    "react-app-polyfill": "^1.0.6",
    "react-testing-library": "^8.0.1",
    "sass-loader": "^8.0.2",
    "serve": "^11.3.0",
    "stylelint": "^13.3.3",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.17.1",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-pwa-manifest": "^4.2.0"
  }
}
